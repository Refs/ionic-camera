    "version": "0.2.0",
    "configurations": [
      {
        "name": "Serve to the browser (ionic serve)",
        "type": "cordova",
        "request": "launch",
        "platform": "serve",
        "devServerAddress": "localhost",
        "sourceMaps": true,
        "ionicLiveReload": true,
        "cwd": "${workspaceRoot}",
        "devServerTimeout":200000
      },
      {
        "name": "Simulate android in browser",
        "type": "cordova",
        "request": "launch",
        "platform": "android",
        "target": "chrome",
        "simulatePort": 8000,
        "livereload": true,
        "cwd": "${workspaceRoot}",
        "sourceMaps": true
      },
        {
          "type": "chrome",
          "request": "attach",
          "name": "Attach to Chrome",
          "port": 9222,
          "url": "http://localhost:8100",
          "webRoot": "${workspaceFolder}/${workspaceFolderBasename}",
          "sourceMaps": true,
          "skipFiles": [
            "${workspaceFolder}/**/node_modules/**/*.js",
          ]
        }
    ]
}
